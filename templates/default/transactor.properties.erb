protocol=<%= @protocol %>
host=<%= @hostname %>
port=4334

license-key=<%= @license_key %>

<% if(@protocol == 'sql') %>
sql-url=<%= @sql_url %>
sql-user=<%= @sql_user %>
sql-password=<%= @sql_password %>
sql-driver-class=oracle.jdbc.OracleDriver
<% end %>

<% if(@protocol == 'riak') %>
riak-host=<%= @riak_host %>
riak-bucket=<%= @riak_bucket %>
<% end %>

# Recommended settings for -Xmx4g, ongoing usage.
memory-index-threshold=<%= @memory_index_threshold %>
memory-index-max=<%= @memory_index_max %>
object-cache-max=<%= @object_cache_max %>

write-concurrency=<%= @write_concurrency %>
read-concurrency=<%= @read_concurrency %>

<% if(@memcached_hosts) %>
memcached=<%= @memcached_hosts %>
<% end %>

<% if(@metrics_callback) %>
metrics-callback=<%=@metrics_callback%>
<% end %>

